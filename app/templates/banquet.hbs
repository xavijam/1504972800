<div class="Slide-content Slide-content--centered">
  <div class="Slide-contentIcon">
    <div class="Animation js-animation">
      <span class="Animation-comment Animation-comment--right">{{ t 'banquet.comment' }}</span>
    </div>
    <img class="Slide-icon" style="width:70px; background-size:70px 70px;" src="data:image/png;base64," alt="{{ t 'banquet.title' }}" title="{{ t 'banquet.title' }}" />
  </div>
  <h2 class="Color Text-title">{{ t 'banquet.title' }}</h2>
  <p class="Slide-contentParagraph Text Color Text-paragraph u-tSpace--xl">{{{ t 'banquet.desc' }}}</p>
</div>