  <ul class="TravelList pure-g">
    {{#each items}}
      <li class="pure-u-1 pure-u-md-1-2 pure-u-lg-1-3">
        <div class="l-box">
          <div class="TravelList-item">
            <div class="TravelList-itemImage">
              <img class="pure-img" src="{{ imageURL }}" title="{{ t title }}" alt="{{ t title }}"/>
              <span class="TravelList-itemCost">{{ cost }}â‚¬</span>
            </div>
            <div class="TravelList-itemInfo">
              <h4 class="TravelList-itemTitle Text">{{ t title }}</h4>
              <p class="Text Text--paragraph TravelList-itemDesc">{{ t desc }}</p>
            </div>
            <div class="TravelList-itemFooter">
              <p class="TravelList-itemType Text Text--paragraph">{{ t type }}</p>

              {{#ifCond wikipediaURL '!=' null }}
                <a href="{{ t wikipediaURL }}" class="TravelList-itemWikipedia" target="_blank">
                  <i class=" fa fa-wikipedia-w"></i>
                </a>
              {{/ifCond}}

              {{#ifCond itemURL '!=' null }}
                <a href="{{ t itemURL }}" class="TravelList-itemLink" target="_blank">
                  <i class=" fa fa-link"></i>
                </a>
              {{/ifCond}}
            </div>
          </div>
        </div>
      </li>
    {{/each}}
  </ul>
