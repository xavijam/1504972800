<div class="Slide-content">
  <div class="Slide-contentIcon js-comments" data-pt-size="normal" data-pt-scheme="black" data-pt-offset-top="0" data-pt-gravity="false" data-pt-position="top-right" data-pt-trigger="sticky" data-pt-offset-left="-20" data-pt-title="{{ t 'church.comment' }}" data-pt-delay-in=1000>
    <img class="Slide-icon" style="height:70px; background-size:70px 70px;" src="data:image/png;base64," alt="{{ t 'church.title' }}" title="{{ t 'church.title' }}" />
  </div>
  <h2 class="Color Text-title">{{ t 'church.title' }}</h2>
  <p class="Slide-contentParagraph Text Color Text-paragraph u-tSpace--xl">{{{ t 'church.desc' }}}</p>
</div>